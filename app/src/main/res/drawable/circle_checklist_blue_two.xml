<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true" android:state_window_focused="false"
        android:drawable="@drawable/circle_checklist_blue_two_default" />
    <item android:state_checked="false" android:state_window_focused="false"
        android:drawable="@drawable/circle_checklist_blue_two_default" />

    <item android:state_checked="true" android:state_pressed="true"
        android:drawable="@drawable/circle_checklist_blue_two_default" />
    <item android:state_checked="false" android:state_pressed="true"
        android:drawable="@drawable/circle_checklist_blue_two_checked" />

    <item android:state_checked="true" android:state_focused="true"
        android:drawable="@drawable/circle_checklist_blue_two_default" />
    <item android:state_checked="false" android:state_focused="true"
        android:drawable="@drawable/circle_checklist_blue_two_checked" />

    <item android:state_checked="false" android:drawable="@drawable/circle_checklist_blue_two_default" />
    <item android:state_checked="true" android:drawable="@drawable/circle_checklist_blue_two_checked" />

    <item android:drawable="@drawable/circle_checklist_blue_two_default" />
</selector>